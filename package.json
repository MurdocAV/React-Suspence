{
  "name": "react-experimental",
  "version": "0.2.2",
  "homepage": "https://murdocav.github.io/home",
  "description": "A React experimental build for testing upcoming/new features.",
  "repository": "https://github.com/MurdocAV/murdocav.github.io",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "dev": "node server --watch",
    "server": "node server",
    "start": "node server",
    "test": "jest --watchAll",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "express": "^4.16.4",
    "superagent": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "jest": "^24.7.1",
    "react": "0.0.0-experimental-5faf377df",
    "react-dom": "0.0.0-experimental-5faf377df"
  }
}
